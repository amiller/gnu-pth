        if test ".$ex_version" != .; then
            CPPFLAGS="$CPPFLAGS `ex-config --cflags`"
            CFLAGS="$CFLAGS `ex-config --cflags`"
            LDFLAGS="$LDFLAGS `ex-config --ldflags`"
        fi
    else
        if test -d "$with_ex"; then
            found=0
            #   via config script
            for dir in $with_ex/bin $with_ex; do
                if test -f "$dir/ex-config" && test ! -f "$dir/ex-config.in"; then
                    ex_version=`($dir/ex-config --version) 2>/dev/null`
                    if test ".$ex_version" != .; then
                        CPPFLAGS="$CPPFLAGS `$dir/ex-config --cflags`"
                        CFLAGS="$CFLAGS `$dir/ex-config --cflags`"
                        LDFLAGS="$LDFLAGS `$dir/ex-config --ldflags`"
                        found=1
                        break
                    fi
                fi
            done
            #   in standard sub-areas
            if test ".$found" = .0; then
                for dir in $with_ex/include/ex $with_ex/include $with_ex; do
                    if test -f "$dir/ex.h"; then
                        CPPFLAGS="$CPPFLAGS -I$dir"
                        CFLAGS="$CFLAGS -I$dir"
                        found=1
                        break
                    fi
                done
                for dir in $with_ex/lib/ex $with_ex/lib $with_ex; do
                    if test -f "$dir/libex.la" && test -d "$dir/.libs"; then
                        LDFLAGS="$LDFLAGS -L$dir -L$dir/.libs"
                        found=1
                        break
                    elif test -f "$dir/libex.a" || test -f "$dir/libex.so"; then
                        LDFLAGS="$LDFLAGS -L$dir"
                        found=1
                        break
                    fi
                done
            fi
