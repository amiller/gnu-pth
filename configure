gpat=''
spat=''
i=1
while test $i -le 3; do
    gpat="$gpat[^,]*"
    if test $i -eq 3; then
        spat="$spat\\([^,]*\\)"
    else
        spat="$spat[^,]*"
    fi
    if test $i -lt 3; then
        gpat="$gpat,"
        spat="$spat,"
    fi
    i=`expr $i + 1`
