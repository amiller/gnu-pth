  withval=$with_mctx_mth;
case $withval in
    mcsc|sjlj ) mctx_mth=$withval ;;
    * ) as_fn_error $? "invalid mctx method -- allowed: mcsc,sjlj" "$LINENO" 5 ;;
esac

